{
  "name": "vue-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 9045",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node src/api/wpApi.js",
    "test:dev": "npx mocha -w src/*.test.js*",
    "test": "npx mocha src/*.test.js*",
    "test:cov": "npx nyc npx mocha src/*.test.js*"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "mocha": "^10.2.0",
    "node": "^21.1.0",
    "nuxt": "^2.15.8",
    "supertest": "^6.3.3",
    "vue": "^3.2.13",
    "vue-router": "^4.1.5",
    "vuejs-paginate-next": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
